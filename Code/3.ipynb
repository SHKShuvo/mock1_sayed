    "# filter rows of a DataFrame based on a condition\n",
    "\n",
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('data.csv', usecols=['Calories'])\n",
    "\n",
    "df.loc[(df['Calories'] >= 300) & (df['Calories'] <= 400)]\n",
    "\n",
    "df"
